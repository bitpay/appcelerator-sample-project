

Titanium.UI.setBackgroundColor('#000');

var tabGroup=Titanium.UI.createTabGroup();global.tabGroup =tabGroup;




var win=require('main.js').getMain();global.win =win;

var tab1=Titanium.UI.createTab({
title:'Tab 1',
window:win});global.tab1 =tab1,

tabGroup.bottom=-100,


tabGroup.addTab(tab1),


setTimeout(function(){
win.open();

},3e3);